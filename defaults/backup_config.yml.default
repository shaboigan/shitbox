---
backup:
  local:
    enable: true
    destination: /mnt/local/Backups/Shitbox
  rclone:
    enable: true
    destination: google:/Backups/Shitbox
  rsync:
    enable: false
    destination: rsync://somehost.com/Backups/Shitbox
    port: 22
  cron:
    cron_time: weekly
    enable: no
  restore_service:
    user:
    pass:
  misc:
    snapshot: true
